<h1>Listing work_orders</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Ordernumber</th>
    <th>Description</th>
    <th>Scheduleddate</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @work_orders.each do |work_order| %>
  <tr>
    <td><%= work_order.id %></td>
    <td><%= work_order.orderNumber %></td>
    <td><%= work_order.description %></td>
    <td><%= work_order.scheduledDate %></td>
    <td><%= link_to 'Show', work_order %></td>
    <td><%= link_to 'Edit', edit_work_order_path(work_order) %></td>
    <td><%= link_to 'Destroy', work_order, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

