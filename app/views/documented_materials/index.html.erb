<h1>Listing documented_materials</h1>

<table>
  <tr>
    <th>Qty</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @documented_materials.each do |documented_material| %>
  <tr>
    <td><%= documented_material.qty %></td>
    <td><%= link_to 'Show', documented_material %></td>
    <td><%= link_to 'Edit', edit_documented_material_path(documented_material) %></td>
    <td><%= link_to 'Destroy', documented_material, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Documented material', new_documented_material_path %>
